{
  "version": 2,
  "rootDirectory": "backend",

  "installCommand": "pnpm install --filter backend...",
  "buildCommand": "pnpm run build",

  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "github": {
    "enabled": true
  }
}
